<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deleguesapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/graduation"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppreciationActivity"
            android:label="@string/title_activity_appreciation" >
            
        </activity>
        <activity
            android:name=".ContactRepertoireActivity"
            android:label="@string/title_activity_contactrepertoire" >
            
        </activity>
        <activity
            android:name=".ContactManuelActivity"
            android:label="@string/title_activity_contactmanuel" >
            
        </activity>
        
    </application>
<uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
<uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
</manifest>